my_tuple = (19, "Boranich", "Aries", 1.60)
my_birthday = "March 25, 2005"
new_tuple = my_tuple + (my_birthday, "Please add me")
print ("My Tuple:", new_tuple)

if my_birthday in new_tuple:
    print ("Yes, my birthday is in the tuple.")
else:
    print ("No, my birthday is not in the tuple.")

length = len(new_tuple)
print ("length of a tuple:", length)
